{
    "name": "history terminator",
    "description": "clears all recent history",
    "version": "1.0",

    "manifest_version": 2,

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },

    "permissions": ["browsingData"]
}